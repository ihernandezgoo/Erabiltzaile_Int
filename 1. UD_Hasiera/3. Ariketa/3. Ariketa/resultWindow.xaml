<Window x:Class="_3._Ariketa.resultWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_3._Ariketa"
        mc:Ignorable="d"
        Title="resultWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Resultado" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0"/>
        <TextBox x:Name="txtResultado" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200" Margin="10,40,0,0" IsReadOnly="True"/>
        
        <Button x:Name="Garbitu" Content="Garbitu" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="10,80,0,0" Click="Garbitu2"/>
        <Button x:Name="Atera" Content="Atera" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="120,80,0,0" Click="Atera2"/>
    </Grid>
</Window>
